# Web-specific environment overrides for Safe Driving
# GraphQL API endpoint (HTTP and optional WS)
GRAPHQL_ENDPOINT=http://localhost:4000/graphql
NEXT_PUBLIC_GRAPHQL_API_URL=http://localhost:4000/graphql
NEXT_PUBLIC_GRAPHQL_WS_URL=ws://localhost:4000/graphql

# LocalStack client host/port (web browser cannot reach 10.0.2.2)
LOCALSTACK_CLIENT_HOST=localhost
LOCALSTACK_CLIENT_PORT=4566

# Optional: Map/ORS overrides for Flutter Web builds (if ever used)
ORS_BASE_URL=https://api.openrouteservice.org
ORS_API_KEY=
ORS_COUNTRY_BOUNDARY=
TILE_URL_TEMPLATE=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
TILE_SUBDOMAINS=a,b,c
MAP_DEFAULT_CENTER=-18.8792,47.5079
MAP_INITIAL_ZOOM=13
